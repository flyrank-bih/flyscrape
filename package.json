    "typescript": "^5.9.3",
    "vitest": "^4.0.13"
  },
  "dependencies": {
    "@dqbd/tiktoken": "^1.0.22",
    "cheerio": "^1.1.2",
    "lru-cache": "^11.2.2",
    "playwright": "^1.57.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "turndown": "^7.2.2"
  }
