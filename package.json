  "description": "A powerful, modular web scraping and crawling library for Node.js, inspired by crawl4ai. Features stealth mode, LLM extraction, and markdown processing.",
  "keywords": [
    "crawler",
    "scraper",
    "web-scraping",
    "headless-browser",
    "playwright",
    "stealth",
    "llm",
    "extraction",
    "markdown",
    "crawl4ai"
  ],
  "author": "Admir Saheta",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flyrank/flyscrape.git"
  },
  "bugs": {
    "url": "https://github.com/flyrank/flyscrape/issues"
  },
  "homepage": "https://github.com/flyrank/flyscrape#readme",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
